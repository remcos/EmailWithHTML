<?xml version="1.0" encoding="utf-8" ?>
<widget id="EmailWithHTML.widget.EmailWithHTML" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>EmailWithHTML</name>
    <description>Open an email client from a phone with pre-filled fields.</description>

    <icon>

	</icon>
    <properties>
        <property key="toField" type="attribute" required="true">
            <caption>To field</caption>
            <category>Values</category>
            <description>The attribute that contains the 'To' field</description>
			<attributeTypes>
				<attributeType name="String"/>
			</attributeTypes>
        </property>
		<property key="ccField" type="attribute" required="true">
            <caption>CC field</caption>
            <category>Values</category>
            <description>The attribute that contains the 'CC' field</description>
			<attributeTypes>
				<attributeType name="String"/>
			</attributeTypes>
        </property>
		<property key="subjectField" type="attribute" required="true">
            <caption>Subject field</caption>
            <category>Values</category>
            <description>The attribute that contains the email subject</description>
			<attributeTypes>
				<attributeType name="String"/>
			</attributeTypes>
        </property>
		<property key="bodyField" type="attribute" required="true">
            <caption>Body field</caption>
            <category>Values</category>
            <description>The attribute that contains the email body</description>
			<attributeTypes>
				<attributeType name="String"/>
			</attributeTypes>
        </property>
    </properties>
</widget>
